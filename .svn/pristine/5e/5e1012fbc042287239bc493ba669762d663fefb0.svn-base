package cn.cnyirui.ims.entity;

public enum IMSTaskType {

	PRODUCT_CATEGORY(0, "产品分类"), //
	PRODUCT(1, "产品"), //
	ORGANIZATION(2, "组织架构"), //
	EMPLOYEE(3, "员工"), //
	EMPLOYEE_WAGE(4, "员工工资"), //
	SALES_ORDER(5, "上报销量"), //
	OTHER(6, "其它"),;//

	private Integer value;
	private String text;

	private IMSTaskType(Integer value, String text) {
		this.value = value;
		this.text = text;
	}

	/**
	 * value
	 * 
	 * @return value value
	 */
	public Integer getValue() {
		return value;
	}

	/**
	 * value
	 * 
	 * @param value value
	 */
	public void setValue(Integer value) {
		this.value = value;
	}

	/**
	 * text
	 * 
	 * @return text text
	 */
	public String getText() {
		return text;
	}

	/**
	 * text
	 * 
	 * @param text text
	 */
	public void setText(String text) {
		this.text = text;
	}

}
