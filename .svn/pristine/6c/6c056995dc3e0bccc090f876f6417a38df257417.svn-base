/*我的订单 start*/
.Selection_card_con{width:90%;margin:15px auto 20px;border:2px solid #a6d2e8;border-radius:5px;}
.Selection_card_con table{height:30px;width:100%;border-collapse: collapse;}
.Selection_card_con table tr{width:100%;height:30px;}
.Selection_card_con table tr td{color:#43a0d2;font-size:1.2em;text-align: center;position: relative;}
.Selection_card_con table tr td span{position: absolute;left:75%;top:0;width:15px;height:15px;color:#fff;font-size:5px;text-align:center;background:red;border-radius:50%;}
.Selection_card_con table tr td.hover{background:#007cc2;color:#fff;}
.order_list{margin:0 auto;display:none; border-top:1px solid #dcdcdc;border-bottom:1px solid #dcdcdc;}

.order_list .order_list_tab{ width:100%; padding:5px;}
.order_list .order_list_tab div{width:100%;margin:0 auto;  border:1px solid #84C3E2; box-sizing: border-box;border-radius:5px;}
.order_list .order_list_tab span.tabspan{position:relative; height:40px;  line-height: 40px;font-weight: bold;text-align: center;color: #1E90CE;}
.order_list .order_list_tab span.tabspan:nth-child(1){border-radius:5px 0 0 5px; }
.order_list .order_list_tab span.tabspan:nth-child(2):before{content: "";position:absolute;left:0;bottom:0px;  width:1px;height:100%;  background-color: #84C3E2;}
.order_list .order_list_tab span.tabspan:nth-child(2):after{content: "";position:absolute;right:0;bottom:0px;  width:1px;height:100%;  background-color: #84C3E2;}
.order_list .order_list_tab span.tabspan:nth-child(3){border-radius:0 5px 5px 0; }
.order_list .order_list_tab span.order_list_active{background-color:#007CC2;color:#fff;    font-size: 16px;}

.order_list .div_but{ width:100%; padding:0 5px;margin-bottom:5px;}
.order_list .div_but button{border:none;width:100%;color:#fff;}
.order_list .div_but button .iconfont{font-size:1.6em;}
.order_list .div_txt{position:relative; width:100%;height:55px;margin:0px 0px 5px;background-color:#fff}
.order_list .div_txt:after{content: "";position:absolute;left:0;bottom:0px;  width:100%;height:1px;  background-color: #ccc;}
/* .order_list .div_txt:before{content: "";position:absolute;left:0;top:0px;  width:100%;height:1px;  background-color: #ccc;} */
.order_list .div_txt .txt{width:70%;height:44px;line-height: 44px;
    font-size: 16px;border:1px solid #bfbfc1;border-radius:5px;text-indent:10px;  margin: 5px 0;}
.order_list .div_txt button{margin:5px 0 5px 5px;width:60px;height:44px;    line-height: 44px;
    font-size: 18px;border:1px solid #6fb7e0;color:#6fb7e0;background:none;border-radius:5px;}
.order_list ul li{width:100%;height:40px;/*line-height:30px;*/border-bottom:1px solid #ccc;}
.order_list ul li{/* display: block; */}
.order_list ul li .span_left{float:left;font-size:1.2em;text-indent:10px;line-height:40px}
.order_list ul li .span_right{float:right;margin-right:10px;line-height:40px;}
.order_list ul li.li2 .span_left{font-size:1.2em;margin-left:10px;}
.order_list ul li.li2 .span_right{color:#666;}
.order_list ul li.li2 .span_right font{color:#fe7b2d; font-weight: 700;font-size: 1.3em;}
.order_list ul li.li2 .span_left font {color:#b30404;font-weight:bold;}
.order_list ul li .span_red{color:#f64e4e;}
.order_list ul li.li1{height:60px;overflow: hidden;padding: 10px;}
.order_list ul li.li1 .span_left{line-height:50px;}
.order_list ul li.li1 .span_right{line-height:20px;padding-top:3px;display:inline-block;}
.order_list ul li.li1 .span_right p{text-align: right;}
.order_list  .order_list_p{color:#2496ec;font-size:1.4em;margin-top:5px;line-height:35px; }
.order_list  .order_list_p1{font-size:1.3em;}
.order_list .more_but{margin-top:10px;width:100%;}
.order_list .more_but span{float:left;font-size:1.1em;display: inline-block;line-height:30px; }
.order_list .more_but  button{float:right;padding:8px 13px;border:none;color:#fff;font-size:1.2em;border-radius:3px;}
.order_list .div_table{width:100%;border-top:1px solid #ccc;border-bottom:1px solid #ccc;}
.order_list .div_table .div_tr{height:100px;background:#fff;position:relative; margin-bottom: 5px;border-bottom: solid 1px #dcdcdc; padding:10px}


.order_list .div_table .div_tr div{}
.order_list .div_table .div_tr .o_name{ width:100%; text-indent: 25px;}
.order_list .div_table .div_tr .o_je{     text-indent: 25px;}
.order_list .div_table .div_tr div img{width:60px;height:60px;}
.order_list .div_table .div_tr div p{line-height:26px; font-size: 16px; width: 50%;float: left;    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;}
.order_list .div_table .div_tr div p.p1{font-size:1.2em;font-weight: bold;  overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.order_list .div_table .div_tr .div_td{width:100%; font-size:18px; color:#575757}
.order_list .div_table .div_tr .div_td1{width:75%;font-size: 20px; float:left}
.order_list .div_table .div_tr .div_td2{width:25%;text-align:right; float:right;line-height: 32px;}
.order_list .div_table .div_tr .div_td1 s,
.order_list .div_table .div_tr .div_td i{color:#959595;}

.order_list .div_table .div_tr .div_td span{ font-weight:700; color: #f60; font-size:18px;}



.order_list s.salesMoney{display:block; min-width:4rem;}
